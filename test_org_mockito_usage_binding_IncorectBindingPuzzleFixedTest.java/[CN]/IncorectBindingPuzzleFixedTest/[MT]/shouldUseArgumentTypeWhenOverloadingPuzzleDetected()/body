{
  Sub sub=mock(Sub.class);
  setMockWithDowncast(sub);
  say("Hello");
  try {
    verify(sub).say("Hello");
    fail();
  }
 catch (  VerificationError error) {
    String expected="\n" + "Invocation differs from actual" + "\n"+ "Wanted: Sub.say(class java.lang.String)"+ "\n"+ "Actual: Sub.say(class java.lang.Object)";
    assertEquals(expected,error.getMessage());
  }
}
