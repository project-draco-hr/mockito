{
  Class<?> returnType=invocation.getMethod().getReturnType();
  if (returnType == String.class) {
    return "";
  }
 else   if (returnType == Boolean.TYPE) {
    return true;
  }
 else   if (!isMockable(returnType)) {
    return mock(returnType);
  }
 else {
    return defaultValueFor(invocation);
  }
}
