{
  buf.setLength(0);
  if (file != null) {
    buf.append(tab).append("// compiled from: ").append(file).append('\n');
  }
  if (debug != null) {
    buf.append(tab).append("// debug info: ").append(debug).append('\n');
  }
  if (buf.length() > 0) {
    text.add(buf.toString());
  }
  if (cv != null) {
    cv.visitSource(file,debug);
  }
}
