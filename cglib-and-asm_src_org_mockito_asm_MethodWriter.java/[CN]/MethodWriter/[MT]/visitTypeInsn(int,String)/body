{
  Item i=cw.newClassItem(type);
  if (currentBlock != null) {
    if (compute == FRAMES) {
      currentBlock.frame.execute(opcode,code.length,cw,i);
    }
 else     if (opcode == Opcodes.NEW) {
      int size=stackSize + 1;
      if (size > maxStackSize) {
        maxStackSize=size;
      }
      stackSize=size;
    }
  }
  code.put12(opcode,i.index);
}
