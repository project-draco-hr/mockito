{
  buf.setLength(0);
  declareLabel(start);
  declareLabel(end);
  declareLabel(handler);
  buf.append("mv.visitTryCatchBlock(");
  appendLabel(start);
  buf.append(", ");
  appendLabel(end);
  buf.append(", ");
  appendLabel(handler);
  buf.append(", ");
  appendConstant(type);
  buf.append(");\n");
  text.add(buf.toString());
}
