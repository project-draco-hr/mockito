{
  buf.setLength(0);
  for (int i=0; i < labels.length; ++i) {
    declareLabel(labels[i]);
  }
  declareLabel(dflt);
  buf.append("mv.visitTableSwitchInsn(").append(min).append(", ").append(max).append(", ");
  appendLabel(dflt);
  buf.append(", new Label[] {");
  for (int i=0; i < labels.length; ++i) {
    buf.append(i == 0 ? " " : ", ");
    appendLabel(labels[i]);
  }
  buf.append(" });\n");
  text.add(buf.toString());
}
