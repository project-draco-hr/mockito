{
  buf.setLength(0);
  buf.append("mv.visitLocalVariable(");
  appendConstant(name);
  buf.append(", ");
  appendConstant(desc);
  buf.append(", ");
  appendConstant(signature);
  buf.append(", ");
  appendLabel(start);
  buf.append(", ");
  appendLabel(end);
  buf.append(", ").append(index).append(");\n");
  text.add(buf.toString());
}
