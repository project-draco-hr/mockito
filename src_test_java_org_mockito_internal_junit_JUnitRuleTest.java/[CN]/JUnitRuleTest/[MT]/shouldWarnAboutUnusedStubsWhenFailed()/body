{
  try {
    jUnitRule.apply(new Statement(){
      public void evaluate() throws Throwable {
        IMethods mock=Mockito.mock(IMethods.class);
        declareUnusedStub(mock);
        throw new AssertionError("x");
      }
    }
,injectTestCase).evaluate();
