{
  StringBuffer sb=new StringBuffer();
  sb.append('{');
  for (Iterator it=keySet().iterator(); it.hasNext(); ) {
    Object key=it.next();
    sb.append(key);
    sb.append('=');
    sb.append(get(key));
    if (it.hasNext()) {
      sb.append(", ");
    }
  }
  sb.append('}');
  return sb.toString();
}
