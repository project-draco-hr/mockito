{
  simpleMethodOnAMock();
  try {
    verifySimpleMethodOnAMock();
    fail();
  }
 catch (  AssertionError e) {
    String expected="\nArgument(s) are different! Wanted:" + "\niMethods.simpleMethod();" + "\n-> at org.mockitousage.stacktrace.StackTrackeChangingTest.verifySimpleMethodOnAMock(StackTrackeChangingTest.java:29)"+ "\nActual invocation has different arguments:"+ "\niMethods.simpleMethod(\"blah\");"+ "\n-> at org.mockitousage.stacktrace.StackTrackeChangingTest.simpleMethodOnAMock(StackTrackeChangingTest.java:25)"+ "\n";
    assertEquals(expected,e.getMessage());
  }
}
