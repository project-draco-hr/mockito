{
  try {
    mock.arrayMethod(new String[]{"a","b","c"});
    mock.forByte((byte)25);
    verify(mock).threeArgumentMethod(12,new Foo(),"xx");
    fail();
  }
 catch (  WantedButNotInvoked e) {
    assertThat(e).hasMessageContaining("iMethods.threeArgumentMethod(12, foo, \"xx\")").hasMessageContaining("iMethods.arrayMethod([\"a\", \"b\", \"c\"])").hasMessageContaining("iMethods.forByte(25)");
  }
}
