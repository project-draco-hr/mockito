{
  if (new VerificationModeDecoder(this).inOrderMode()) {
    invocations=new AllInvocationsFinder().getAllInvocations(this.getMocksToBeVerifiedInOrder());
  }
  List<Verifier> verifiers=Arrays.asList(new MissingInvocationInOrderVerifier(),new NumberOfInvocationsInOrderVerifier(),new MissingInvocationVerifier(),new NumberOfInvocationsVerifier(),new NoMoreInvocationsVerifier());
  for (  Verifier verifier : verifiers) {
    if (verifier.appliesTo(this)) {
      verifier.verify(invocations,wanted,this);
    }
  }
}
