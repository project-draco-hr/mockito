{
  if (check && !(contains(location) && insn.index == -1)) {
    throw new IllegalArgumentException();
  }
  ++size;
  AbstractInsnNode next=location.next;
  if (next == null) {
    last=insn;
  }
 else {
    next.prev=insn;
  }
  location.next=insn;
  insn.next=next;
  insn.prev=location;
  cache=null;
  insn.index=0;
}
