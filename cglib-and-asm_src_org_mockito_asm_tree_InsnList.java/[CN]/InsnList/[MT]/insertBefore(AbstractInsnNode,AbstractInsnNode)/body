{
  if (check && !(contains(location) && insn.index == -1)) {
    throw new IllegalArgumentException();
  }
  ++size;
  AbstractInsnNode prev=location.prev;
  if (prev == null) {
    first=insn;
  }
 else {
    prev.next=insn;
  }
  location.prev=insn;
  insn.next=location;
  insn.prev=prev;
  cache=null;
  insn.index=0;
}
