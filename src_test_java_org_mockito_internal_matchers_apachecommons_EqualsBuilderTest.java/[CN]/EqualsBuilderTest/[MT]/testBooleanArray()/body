{
  boolean[] obj1=new boolean[2];
  obj1[0]=true;
  obj1[1]=false;
  boolean[] obj2=new boolean[2];
  obj2[0]=true;
  obj2[1]=false;
  assertTrue(new EqualsBuilder().append(obj1,obj1).isEquals());
  assertTrue(new EqualsBuilder().append(obj1,obj2).isEquals());
  obj1[1]=true;
  assertTrue(!new EqualsBuilder().append(obj1,obj2).isEquals());
  obj2=null;
  assertTrue(!new EqualsBuilder().append(obj1,obj2).isEquals());
  obj1=null;
  assertTrue(new EqualsBuilder().append(obj1,obj2).isEquals());
}
