{
  float[] obj1=new float[2];
  obj1[0]=5;
  obj1[1]=6;
  float[] obj2=new float[2];
  obj2[0]=5;
  obj2[1]=6;
  assertTrue(new EqualsBuilder().append(obj1,obj1).isEquals());
  assertTrue(new EqualsBuilder().append(obj1,obj2).isEquals());
  obj1[1]=7;
  assertTrue(!new EqualsBuilder().append(obj1,obj2).isEquals());
  obj2=null;
  assertTrue(!new EqualsBuilder().append(obj1,obj2).isEquals());
  obj1=null;
  assertTrue(new EqualsBuilder().append(obj1,obj2).isEquals());
}
