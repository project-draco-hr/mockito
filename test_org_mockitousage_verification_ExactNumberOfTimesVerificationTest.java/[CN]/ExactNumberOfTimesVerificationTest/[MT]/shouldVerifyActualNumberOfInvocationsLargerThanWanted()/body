{
  mock.clear();
  mock.clear();
  mock.clear();
  Mockito.verify(mock,3).clear();
  try {
    Mockito.verify(mock,1).clear();
    fail();
  }
 catch (  NumberOfInvocationsError e) {
    String expected="\n" + "LinkedList.clear()" + "\n"+ "Wanted 1 time but was 3";
    assertEquals(expected,e.getMessage());
  }
}
