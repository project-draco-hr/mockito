{
  assertContainsIgnoringCase("foo","foo");
  assertContainsIgnoringCase("fOo","foo");
  assertContainsIgnoringCase("FoO","foo");
  assertContainsIgnoringCase("foo","a foo :)");
  assertContainsIgnoringCase("fOo","a foo :)");
  assertContainsIgnoringCase("FoO","a foo :)");
  assertContainsIgnoringCase("","a foo :)");
  assertContainsIgnoringCase("","");
}
