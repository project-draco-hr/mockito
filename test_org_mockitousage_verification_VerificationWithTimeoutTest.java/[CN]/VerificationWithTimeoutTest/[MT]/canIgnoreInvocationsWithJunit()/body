{
  Thread t1=new Thread(){
    @Override public void run(){
      mock.add("0");
      mock.add("1");
      try {
        Thread.sleep(100);
      }
 catch (      InterruptedException ignored) {
      }
      mock.add("2");
    }
  }
;
  t1.start();
  verify(mock,timeout(200)).add("1");
  verify(mock,timeout(200)).add("2");
}
