{
  for (  Object m : mocks) {
    InvocationContainer invocationContainer=new MockUtil().getMockHandler(m).getInvocationContainer();
    List<InvocationImpl> ins=invocationContainer.getInvocations();
    for (    InvocationImpl in : ins) {
      if (in.stubInfo() != null) {
        in.ignoreForVerification();
      }
    }
  }
  return mocks;
}
