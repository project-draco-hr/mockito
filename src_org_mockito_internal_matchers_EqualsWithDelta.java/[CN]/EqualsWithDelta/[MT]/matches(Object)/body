{
  Number actualNumber=(Number)actual;
  if (wanted == null ^ actual == null) {
    return false;
  }
  if (wanted == null && actual == null) {
    return true;
  }
  return wanted.doubleValue() - delta.doubleValue() <= actualNumber.doubleValue() && actualNumber.doubleValue() <= wanted.doubleValue() + delta.doubleValue();
}
