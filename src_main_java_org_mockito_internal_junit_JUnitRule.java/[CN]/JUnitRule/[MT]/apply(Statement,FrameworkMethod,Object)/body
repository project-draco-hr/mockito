{
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      WarningsCollector c=new WarningsCollector();
      MockitoAnnotations.initMocks(target);
      try {
        base.evaluate();
      }
 catch (      Throwable t) {
        logger.log(c.getWarnings());
        throw t;
      }
      Mockito.validateMockitoUsage();
    }
  }
;
}
