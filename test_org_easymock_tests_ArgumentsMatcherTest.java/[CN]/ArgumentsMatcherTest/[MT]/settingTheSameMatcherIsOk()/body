{
  try {
    mock.twoArgumentMethod(1,2);
    control.setMatcher(MockControl.ARRAY_MATCHER);
    control.setMatcher(MockControl.ARRAY_MATCHER);
    mock.twoArgumentMethod(1,2);
    control.setMatcher(MockControl.ARRAY_MATCHER);
  }
 catch (  IllegalStateException unexpected) {
    fail("no exception should be thrown if the same matcher is set twice");
  }
}
