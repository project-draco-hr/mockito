{
  return new Assertor<Collection>(){
    public void assertValue(    Collection value){
      int matched=0;
      for (      Matcher<T> m : elements) {
        for (        Object el : value) {
          if (m.matches(el)) {
            matched++;
            continue;
          }
        }
      }
      assertEquals("At least one of the matchers failed to match any of the elements",elements.length,matched);
    }
  }
;
}
