{
  Boo boo=mock(Boo.class);
  boo.withLongAndInt(100,200);
  try {
    verify(boo).withLongAndInt(eq(100),eq(200));
    fail();
  }
 catch (  ArgumentsAreDifferent e) {
    assertThat(e).hasMessageContaining("withLongAndInt((Integer) 100, 200)").hasMessageContaining("withLongAndInt((Long) 100, 200)");
  }
}
