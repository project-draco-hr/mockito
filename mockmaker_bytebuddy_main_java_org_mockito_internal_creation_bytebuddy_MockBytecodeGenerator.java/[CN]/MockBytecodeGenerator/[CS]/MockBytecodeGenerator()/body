{
  byteBuddy=new ByteBuddy(ClassFileVersion.JAVA_V5).withDefaultMethodAttributeAppender(MethodAttributeAppender.ForInstrumentedMethod.INSTANCE).withAttribute(TypeAttributeAppender.ForSuperType.INSTANCE);
  toDispatcher=MethodDelegation.to(DispatcherDefaultingToRealMethod.class).appendParameterBinder(FieldProxy.Binder.install(FieldGetter.class,FieldSetter.class));
  random=new Random();
}
