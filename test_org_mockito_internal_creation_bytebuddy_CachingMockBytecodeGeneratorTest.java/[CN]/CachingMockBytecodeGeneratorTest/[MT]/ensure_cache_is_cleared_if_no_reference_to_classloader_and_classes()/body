{
  ClassLoader classloader_with_life_shorter_than_cache=inMemoryClassLoader().withClassDefinition("foo.Bar",makeMarkerInterface("foo.Bar")).build();
  CachingMockBytecodeGenerator cachingMockBytecodeGenerator=new CachingMockBytecodeGenerator();
  Class<?> the_mock_type=cachingMockBytecodeGenerator.get(classloader_with_life_shorter_than_cache.loadClass("foo.Bar"),Collections.<Class>emptySet());
  assumeTrue(cachingMockBytecodeGenerator.avoidingClassLeakageCache.size() == 1);
  classloader_with_life_shorter_than_cache=is_no_more_referenced();
  the_mock_type=is_no_more_referenced();
  System.gc();
  ensure_gc_happened();
  assertThat(cachingMockBytecodeGenerator.avoidingClassLeakageCache).isEmpty();
}
