{
  return new Assertor<Object>(){
    public void assertValue(    Object o){
      Class<?> clazz=null;
      if (o instanceof Class) {
        clazz=(Class<?>)o;
      }
 else {
        clazz=o.getClass();
      }
      for (      Method m : clazz.getMethods()) {
        if (m.isBridge() && m.getName().equals(methodName)) {
          return;
        }
      }
      fail("Bridge method [" + methodName + "]\nnot found in:\n"+ o);
    }
  }
;
}
