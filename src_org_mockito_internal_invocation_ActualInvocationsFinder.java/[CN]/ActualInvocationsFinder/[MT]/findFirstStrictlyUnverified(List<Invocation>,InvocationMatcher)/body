{
  List<Invocation> unverified=new LinkedList<Invocation>();
  for (  Invocation i : invocations) {
    if (i.isVerifiedStrictly()) {
      continue;
    }
    if (wanted.matches(i)) {
      unverified.add(i);
      continue;
    }
    if (unverified.isEmpty()) {
      unverified.add(i);
    }
    break;
  }
  return unverified;
}
