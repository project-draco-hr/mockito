{
  if (fastClassInfo == null) {
synchronized (initLock) {
      if (fastClassInfo == null) {
        CreateInfo ci=createInfo;
        FastClassInfo fci=new FastClassInfo();
        fci.f1=helper(ci,ci.c1);
        fci.f2=helper(ci,ci.c2);
        fci.i1=fci.f1.getIndex(sig1);
        fci.i2=fci.f2.getIndex(sig2);
        fastClassInfo=fci;
      }
    }
  }
}
