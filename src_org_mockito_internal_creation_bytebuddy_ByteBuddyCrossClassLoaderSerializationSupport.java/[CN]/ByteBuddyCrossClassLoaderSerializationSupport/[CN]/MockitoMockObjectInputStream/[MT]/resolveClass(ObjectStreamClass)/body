{
  if (notMarkedAsAMockitoMock(readObject())) {
    return super.resolveClass(desc);
  }
  Class<?> proxyClass=((ByteBuddyMockMaker)Plugins.getMockMaker()).createProxyClass(withMockFeatures(typeToMock,extraInterfaces,true));
  hackClassNameToMatchNewlyCreatedClass(desc,proxyClass);
  return proxyClass;
}
