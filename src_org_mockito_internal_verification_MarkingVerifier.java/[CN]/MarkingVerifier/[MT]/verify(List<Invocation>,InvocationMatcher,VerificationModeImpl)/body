{
  if (!mode.explicitMode()) {
    return;
  }
  List<Invocation> actualInvocations=finder.findInvocations(invocations,wanted,mode);
  for (  Invocation invocation : actualInvocations) {
    invocation.markVerified();
    if (mode.strictMode()) {
      invocation.markVerifiedStrictly();
    }
  }
}
