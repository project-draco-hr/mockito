{
  if (TypeUtils.isPrimitive(type)) {
    if (type == Type.VOID_TYPE) {
      aconst_null();
    }
 else {
      Type boxed=TypeUtils.getBoxedType(type);
      new_instance(boxed);
      if (type.getSize() == 2) {
        dup_x2();
        dup_x2();
        pop();
      }
 else {
        dup_x1();
        swap();
      }
      invoke_constructor(boxed,new Signature(Constants.CONSTRUCTOR_NAME,Type.VOID_TYPE,new Type[]{type}));
    }
  }
}
