{
switch (sort) {
case VOID:
    buf.append('V');
  return;
case BOOLEAN:
buf.append('Z');
return;
case CHAR:
buf.append('C');
return;
case BYTE:
buf.append('B');
return;
case SHORT:
buf.append('S');
return;
case INT:
buf.append('I');
return;
case FLOAT:
buf.append('F');
return;
case LONG:
buf.append('J');
return;
case DOUBLE:
buf.append('D');
return;
case ARRAY:
buf.append(this.buf,off,len);
return;
default :
buf.append('L');
buf.append(this.buf,off,len);
buf.append(';');
}
}
