{
  StringBuilder sb=new StringBuilder();
  int counter=0;
  for (  VerificationAwareInvocation i : invocations) {
    sb.append(++counter + ". ");
    if (!i.isVerified()) {
      sb.append("[?]");
    }
    sb.append(i.getLocation() + "\n");
  }
  String scenario=sb.toString();
  return scenario;
}
