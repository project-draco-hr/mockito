{
  if (o == null || !this.getClass().equals(o.getClass()))   return false;
  InvocationWithMatchers other=(InvocationWithMatchers)o;
  return this.invocation.equals(other.invocation) && ((this.matchers == null && other.matchers == null) || (this.matchers != null && this.matchers.equals(other.matchers)));
}
