{
  if (prefix == null) {
    prefix="org.mockito.cglib.empty.Object";
  }
 else   if (prefix.startsWith("java")) {
    prefix="$" + prefix;
  }
  String base=prefix + "$$" + source.substring(source.lastIndexOf('.') + 1)+ getTag()+ "$$"+ Integer.toHexString(key.hashCode());
  String attempt=base;
  int index=2;
  while (names.evaluate(attempt))   attempt=base + "_" + index++;
  return attempt;
}
