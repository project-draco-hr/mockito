{
  buf.setLength(0);
  appendComa(valueNumber++);
  if (name != null) {
    buf.append(name).append('=');
  }
  buf.append('{');
  text.add(buf.toString());
  TraceAnnotationVisitor tav=createTraceAnnotationVisitor();
  text.add(tav.getText());
  text.add("}");
  if (av != null) {
    tav.av=av.visitArray(name);
  }
  return tav;
}
