{
  InternalMockHandler mockitoHandler=cast(handler);
  new AcrossJVMSerializationFeature().enableSerializationAcrossJVM(settings);
  return new ClassImposterizer(new InstantiatorProvider().getInstantiator()).imposterise(new MethodInterceptorFilter(mockitoHandler,settings),settings.getTypeToMock(),settings.getExtraInterfaces());
}
