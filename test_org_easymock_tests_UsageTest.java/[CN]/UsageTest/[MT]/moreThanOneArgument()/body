{
  mock.threeArgumentMethod(1,"2","3");
  control.setReturnValue("Test",2);
  control.replay();
  assertEquals("Test",mock.threeArgumentMethod(1,"2","3"));
  boolean failed=true;
  try {
    control.verify();
    failed=false;
  }
 catch (  AssertionError expected) {
    assertEquals("\n  Expectation failure on verify:" + "\n    threeArgumentMethod(1, \"2\", \"3\"): expected: 2, actual: 1",expected.getMessage());
  }
  if (!failed) {
    fail("exception expected");
  }
}
