{
  mock.oneArg(false);
  control.setReturnValue("Test");
  control.setReturnValue("Test2");
  control.setThrowable(new IndexOutOfBoundsException(),MockControl.ONE_OR_MORE);
  control.replay();
  assertEquals("Test",mock.oneArg(false));
  assertEquals("Test2",mock.oneArg(false));
  try {
    mock.oneArg(false);
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    mock.oneArg(false);
  }
 catch (  IndexOutOfBoundsException expected) {
  }
}
