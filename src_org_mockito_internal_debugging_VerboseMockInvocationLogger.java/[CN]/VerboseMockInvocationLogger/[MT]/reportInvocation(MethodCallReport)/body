{
  if (methodCallReport.threwException()) {
    onCallWithThrowable(methodCallReport.getInvocation(),methodCallReport.getThrowable(),methodCallReport.getLocationOfStubbing());
  }
 else {
    onNormalCall(methodCallReport.getInvocation(),methodCallReport.getReturnedValue(),methodCallReport.getLocationOfStubbing());
  }
}
