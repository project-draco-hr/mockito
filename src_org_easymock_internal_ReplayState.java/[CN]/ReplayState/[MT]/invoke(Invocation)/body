{
  Result result=behavior.addActual(invocation);
  LastArguments.pushCurrentArguments(invocation.getArguments());
  try {
    try {
      return result.answer();
    }
 catch (    Throwable t) {
      throw new ThrowableWrapper(t);
    }
  }
  finally {
    LastArguments.popCurrentArguments();
  }
}
