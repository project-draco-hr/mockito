{
  mock.simpleMethod();
  try {
    verify(mock).simpleMethod("test");
    fail();
  }
 catch (  VerificationError expected) {
    String actualMessage=expected.getMessage();
    String expectedMessage="\n" + "Invocation differs from actual" + "\n"+ "Wanted: IMethods.simpleMethod(\"test\")"+ "\n"+ "Actual: IMethods.simpleMethod()";
    assertEquals(expectedMessage,actualMessage);
  }
}
