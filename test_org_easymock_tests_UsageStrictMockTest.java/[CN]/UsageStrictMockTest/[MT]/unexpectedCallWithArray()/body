{
  control.reset();
  control.setDefaultMatcher(MockControl.ARRAY_MATCHER);
  mock.arrayMethod(new String[]{"Test","Test 2"});
  control.replay();
  boolean failed=false;
  String[] strings=new String[]{"Test"};
  try {
    mock.arrayMethod(strings);
  }
 catch (  AssertionError expected) {
    failed=true;
    assertEquals("\n  Unexpected method call arrayMethod(" + strings.toString() + "):"+ "\n    arrayMethod([\"Test\", \"Test 2\"]): expected: 1, actual: 0",expected.getMessage());
  }
  if (!failed) {
    fail("exception expected");
  }
}
