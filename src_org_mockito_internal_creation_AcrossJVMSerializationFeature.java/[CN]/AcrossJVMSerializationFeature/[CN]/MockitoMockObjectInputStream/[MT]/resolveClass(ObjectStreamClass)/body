{
  if (notMarkedAsAMockitoMock(readObject())) {
    return super.resolveClass(desc);
  }
  CglibClassImposterizer imposterizer=new CglibClassImposterizer(new InstantiatorProvider().getInstantiator());
  imposterizer.setConstructorsAccessible(typeToMock,true);
  Class<?> proxyClass=imposterizer.createProxyClass(typeToMock,extraInterfaces.toArray(new Class[extraInterfaces.size()]));
  hackClassNameToMatchNewlyCreatedClass(desc,proxyClass);
  return proxyClass;
}
