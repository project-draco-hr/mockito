{
  StringBuffer sb=new StringBuffer();
  for (int i=0, len=s.length(); i < len; i++) {
    char c=s.charAt(i);
switch (c) {
case '$':
      sb.append("$24");
    break;
case '.':
  sb.append("$2E");
break;
case '[':
sb.append("$5B");
break;
case ';':
sb.append("$3B");
break;
case '(':
sb.append("$28");
break;
case ')':
sb.append("$29");
break;
case '/':
sb.append("$2F");
break;
default :
sb.append(c);
}
}
return sb.toString();
}
