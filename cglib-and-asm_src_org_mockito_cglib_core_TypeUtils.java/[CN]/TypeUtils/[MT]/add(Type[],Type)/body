{
  if (types == null) {
    return new Type[]{extra};
  }
 else {
    List list=Arrays.asList(types);
    if (list.contains(extra)) {
      return types;
    }
    Type[] copy=new Type[types.length + 1];
    System.arraycopy(types,0,copy,0,types.length);
    copy[types.length]=extra;
    return copy;
  }
}
