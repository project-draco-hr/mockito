{
  Throwable t=new TraceBuilder().classes("org.test.MockitoSampleTest","org.test.TestSupport","org.mockito.Mockito","org.test.TestSupport","org.mockito.Mockito").toThrowable();
  filter.filterStackTrace(t);
  assertThat(t,hasOnlyThoseClassesInStackTrace("org.test.TestSupport","org.test.MockitoSampleTest"));
}
