{
  if (ignoreCommit.isTrue(commit)) {
    return;
  }
  commits.add(commit);
  tickets.addAll(commit.getTickets());
  Contribution c=contributions.get(commit.getAuthorId());
  if (c == null) {
    contributions.put(commit.getAuthorId(),new Contribution(commit));
  }
 else {
    c.add(commit);
  }
}
