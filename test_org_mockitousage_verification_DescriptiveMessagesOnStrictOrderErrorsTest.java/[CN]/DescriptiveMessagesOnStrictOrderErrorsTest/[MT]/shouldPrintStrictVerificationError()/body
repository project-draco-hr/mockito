{
  try {
    strictly.verify(one).simpleMethod(999);
    fail();
  }
 catch (  VerificationError e) {
    String expected="\n" + "Invocation differs from actual" + "\n"+ "Wanted invocation:"+ "\n"+ "IMethods.simpleMethod(999)";
    assertEquals(expected,e.getMessage());
    assertEquals(e.getCause().getClass(),WantedDiffersFromActual.class);
    String expectedCause="\n" + "Actual invocation:" + "\n"+ "IMethods.simpleMethod(1)";
    assertEquals(expectedCause,e.getCause().getMessage());
  }
}
