{
  mock.oneArg(true);
  mock.oneArg(false);
  mock.threeArgumentMethod(1,"2","3");
  verify(mock).oneArg(true);
  try {
    verifyNoMoreInteractions(mock);
    fail();
  }
 catch (  VerificationError expected) {
    String actualMessage=expected.getMessage();
    String expectedMessage="\n" + "No more interactions wanted" + "\n"+ "Unexpected: IMethods.oneArg(false)";
    assertEquals(expectedMessage,actualMessage);
  }
}
