{
  Invocation firstSimilar=null;
  for (  Invocation invocation : invocations) {
    if (!wanted.hasSimilarMethod(invocation)) {
      continue;
    }
    if (firstSimilar == null) {
      firstSimilar=invocation;
    }
    if (wanted.hasSameMethod(invocation)) {
      return invocation;
    }
  }
  return firstSimilar;
}
