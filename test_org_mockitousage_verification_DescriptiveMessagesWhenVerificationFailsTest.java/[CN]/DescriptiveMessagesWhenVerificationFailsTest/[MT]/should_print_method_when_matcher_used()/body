{
  try {
    verify(mock,atLeastOnce()).twoArgumentMethod(anyInt(),eq(100));
    fail();
  }
 catch (  WantedButNotInvoked e) {
    String actualMessage=e.getMessage();
    String expectedMessage="\n" + "Wanted but not invoked:" + "\n"+ "iMethods.twoArgumentMethod(\n"+ "    isA(java.lang.Integer),\n"+ "    100\n"+ ");";
    assertContains(expectedMessage,actualMessage);
  }
}
