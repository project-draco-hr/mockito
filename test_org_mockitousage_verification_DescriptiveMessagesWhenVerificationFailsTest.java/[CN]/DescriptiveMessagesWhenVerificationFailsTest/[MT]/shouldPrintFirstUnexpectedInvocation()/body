{
  mock.oneArg(true);
  mock.oneArg(false);
  mock.threeArgumentMethod(1,"2","3");
  verify(mock).oneArg(true);
  try {
    verifyNoMoreInteractions(mock);
    fail();
  }
 catch (  NoInteractionsWanted e) {
    String expectedMessage="\n" + "No interactions wanted here:" + "\n"+ "-> at";
    assertContains(expectedMessage,e.getMessage());
    String expectedCause="\n" + "But found this interaction:" + "\n"+ "-> at";
    assertContains(expectedCause,e.getMessage());
  }
}
