{
  List<Field> unsortedFields=Arrays.asList(field("objectB"),field("integerB"),field("numberA"),field("numberB"),field("objectA"),field("integerA"));
  Collections.sort(unsortedFields,cmp);
  assertThat(unsortedFields).containsSequence(field("integerA"),field("integerB"),field("numberA"),field("numberB"),field("objectA"),field("objectB"));
}
